{
    "name": "black-panther-front-end",
    "version": "0.1.0",
    "private": true,
    "proxy": "http://localhost:5000",
    "dependencies": {
        "bootstrap": "^3.3.7",
        "date-fns": "^1.29.0",
        "lodash": "^4.17.11",
        "node-sass-chokidar": "^1.3.3",
        "npm-run-all": "^4.1.3",
        "react": "^16.5.0",
        "react-bootstrap": "^0.32.4",
        "react-cookie": "^3.0.4",
        "react-dom": "^16.5.0",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.3.1",
        "react-scripts": "1.1.5",
        "redux": "^4.0.0",
        "redux-saga": "^0.16.1"
    },
    "scripts": {
        "start": "react-app-rewired start",
        "build": "export PUBLIC_URL=\"/app\" && react-app-rewired build",
        "test": "react-app-rewired test --env=jsdom",
        "eject": "react-app-rewired eject",
        "storybook": "start-storybook -p 9009 -s public",
        "build-storybook": "build-storybook -s public",
        "format": "prettier --write -l --config=\"../.prettierrc.json\" src/**/*.{js,json,css,md}",
        "lint": "./node_modules/.bin/eslint src",
        "precommit": "lint-staged"
    },
    "devDependencies": {
        "@storybook/addon-actions": "^3.4.11",
        "@storybook/addon-backgrounds": "^3.4.11",
        "@storybook/addon-knobs": "^3.4.11",
        "@storybook/addon-links": "^3.4.11",
        "@storybook/addon-notes": "^3.4.11",
        "@storybook/addon-viewport": "^3.4.11",
        "@storybook/addons": "^3.4.11",
        "@storybook/react": "^3.4.11",
        "babel-core": "^6.26.3",
        "babel-eslint": "^9.0.0",
        "babel-loader": "^8.0.2",
        "babel-runtime": "^6.26.0",
        "eslint": "^5.6.0",
        "eslint-loader": "^2.1.0",
        "eslint-plugin-react": "^7.11.1",
        "node-sass": "^4.9.3",
        "react-app-rewire-css-modules": "github:codebandits/react-app-rewire-css-modules",
        "react-app-rewired": "^1.6.2",
        "sass-loader": "^7.1.0",
        "husky": "^0.14.3",
        "lint-staged": "^7.0.4",
        "prettier": "^1.11.1"
    },
    "lint-staged": {
        "*.{js,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    }
}
