{
    "parser": "babel-eslint",
    "extends": "react-app",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
      },
      "rules": {
        "no-underscore-dangle": 0,
        "import/no-extraneous-dependencies": "off",
        "import/no-unresolved" : "off",
        "import/extensions": "off",
        "react/prop-types": "off",
        "func-names": "off",
        "max-len": [2, {"code": 200}],
        "indent": "off",
        "quotes": [
          "error",
          "single"
        ],
        "semi": [
          "error",
          "always"
        ],
        "comma-dangle": [1, "always-multiline"]
      },
      "plugins": [
        "react"
      ]
}